{
  "editor.unicodeHighlight.nonBasicASCII": false,
  "terminal.integrated.commandsToSkipShell": ["matlab.interrupt"],
  "omnisharp.useModernNet": false,
  "editor.tabSize": 2,
  "workbench.iconTheme": "material-icon-theme",
  "editor.fontFamily": "JetBrainsMono Nerd Font, Consolas, 'Courier New', monospace",
  "files.watcherExclude": {
    "**/.git/**": true,
    "**/.node_modules/**": true,
    "**/dist/**": true,
    "**/out-tsc/**": true
  },
  "typescript.suggest.paths": false,
  "javascript.suggest.paths": false,
  "editor.formatOnSave": true,
  "editor.formatOnSaveMode": "modificationsIfAvailable",
  "editor.cursorSmoothCaretAnimation": "on",
  "workbench.colorCustomizations": {
    "terminal.background": "#00000000"
  },
  "workbench.settings.applyToAllProfiles": ["workbench.colorCustomizations"],
  "window.titleBarStyle": "custom",
  "vscode_vibrancy.opacity": 0.5,
  "synthwave84.brightness": 0.15,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.foldingImportsByDefault": true,
  "editor.guides.bracketPairs": "active",
  "editor.linkedEditing": true,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "phase",
  "editor.fontLigatures": true,
  "editor.inlineSuggest.syntaxHighlightingEnabled": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "workbench.commandPalette.experimental.suggestCommands": true,
  "workbench.list.smoothScrolling": true,
  "workbench.reduceMotion": "off",
  "workbench.view.showQuietly": {
    "workbench.panel.output": false
  },
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabsOnSeparateRow": true,
  "workbench.editor.wrapTabs": true,
  "window.menuBarVisibility": "compact",
  "explorer.fileNesting.enabled": true,
  "debug.autoExpandLazyVariables": "on",
  "debug.showVariableTypes": true,
  "telemetry.telemetryLevel": "off",
  "angular.view-engine": true,
  "workbench.colorTheme": "SynthWave '84",
  "editor.minimap.autohide": true,
  "files.autoSave": "afterDelay",
  "files.insertFinalNewline": true,
  "update.mode": "start",
  "git.autofetch": true,
  "git.blame.editorDecoration.enabled": true,
  "git.blame.statusBarItem.enabled": true,
  "sqldeveloper.sqlHistory.historyLimit": 500,
  "sqldeveloper.telemetry.enabled": false,
  "mssql.connections": [
    {
      "server": "ASXVPWARK013022",
      "database": "",
      "authenticationType": "SqlLogin",
      "user": "sa",
      "password": "",
      "emptyPasswordInput": false,
      "savePassword": true,
      "profileName": "Production",
      "encrypt": "Mandatory",
      "trustServerCertificate": true,
      "connectTimeout": 15,
      "commandTimeout": 30,
      "applicationName": "vscode-mssql"
